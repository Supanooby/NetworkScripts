!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!R1
en
conf t
Hostname R1
no ip domain lookup 
enable secret class 

line con 0
password cisco
login

line vty 0 4 
password cisco 
login

service password-encryption

banner motd $ Authorized Users Only! $

!intervlan route
inte g0/0/1 
no shut 

inte g0/0/1.20
description management Network
ecapsulation dot1q 20
ip address 10.20.0.1 255.255.255.0


int g0/0/1.30
encapsulation dot1q 30
description Operations Network
ip address 10.30.0.1 255.255.255.0

int g0/0/1.40
encapsulation dot1q 40
description Sales Network
ip address 10.40.0.1 255.255.255.0

inte g0/0/1.1000
encapsulation dot1q 1000 native
description Native VLAN

inte lo1
ip address 172.16.1.1 255.255.255.0







!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!R2
en
conf t
Hostname R2
no ip domain lookup 
enable secret class 

line con 0
password cisco
login

line vty 0 4 
password cisco 
login

service password-encryption

banner motd $ Authorized Users Only! $



!Configure g0/0/1 and set default route 
inte g0/0/1 
ip address 10.20.0.4 255.255.255.0
no shut
exit

ip route 0.0.0.0 0.0.0.0 10.20.0.1


!Configure SSH
username SSHadmin secret $cisco123!
ip domain name ccna-lab.com
crypto key generate rsa general-keys modulus 1024

line vty 0 4 
transport input ssh
login local
exit






!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! S1
Hostname S1
no ip domain-lookup
enable secret class

line con 0
password cisco
login

line vty 0 4
password cisco
login

service password-encryption
banner motd $ Authorized Users Only! $

! Create Vlans
vlan 20
name Management 
vlan 30
name Operations
vlan 40
name Sales
vlan 999
name Parkinglot
vlan 1000
name Native
exit

!Configure management interface + default gateway
inte vlan 20
ip address 10.20.0.2 255.255.255.0
no shut
exit
ip default-gateway 10.20.0.1
end

!Move all unused ports to parkinglot vlan, set static access, shut down
inte range f0/2 - 4, f0/7 - 24, g0/1 - 2 
switchport mode acces 
switchport access vlan 999
shut
end

!assign interface to corect vlan
inte f0/6
switchport mode access
switchport access vlan 30

!Configure trunking
inte f0/1
switchport mode trunk
switchport trunk native vlan 1000
switchport trunk allowed vlan 20,30,40,1000

!trunk to r1
inte f0/5
switchport mode trunk
switchport trunk native vlan 1000
sitchport trunk allowed vlan 20,30,40,1000




